<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - QA_AUDIT_TEST</title>
    <link href="~/css/site.css" rel="stylesheet" />
    <script src="~/js/site.js"></script>
    <a href="~/lib/bootstrap/dist/css/bootstrap.min.css.map"></a>
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <a href="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js.map"></a>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    @* User-defined dependencies *@
    <script src="~/lib/jquery-ajax-unobtrusive/jquery.unobtrusive-ajax.min.js"></script>
    <link href="~/css/homepage.css" rel="stylesheet" />
    <link href="~/css/dashboard.css" rel="stylesheet" />
    <script src="~/chart.js/chart.js"></script>
    <script src="~/datatables/datatable.min.js"></script>
    <script src="https://kit.fontawesome.com/b8fb466bb6.js" crossorigin="anonymous"></script>

</head>
<body>
    <header class="bg-primary">
        <div class="w-100 d-flex justify-content-between align-self-start bg-primary">
            <div class="my-auto">
                <img style="width:60px; height: 30px" src="~/pimeslogo.png" />
            </div>
            <div class="">
                <h3 class="text-white fs-3 my-auto text-center fw-bolder">ATS Quality Assurance - TEST</h3>
            </div>
            <div class="my-auto">
                <img style="width:60px; height: 30px" src="~/ats wbg.png" />
            </div>
        </div>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-primary border-bottom box-shadow mb-3">
            <div class="container-fluid">   
                <a class="navbar-brand text-white fw-bolder" asp-area="" asp-controller="Home" asp-action="Index">AUDIT PORTAL</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Homepage">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    @* <footer class="border-top text-muted">
        <div class="container-fluid">
            &copy; 2024 - QA_AUDIT - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer> *@
</body>
</html>
@await RenderSectionAsync("Scripts", required: true)
