@model DMD_Prototype.Controllers.MTIViewModel
@{
    Layout = null;
    ViewData["Title"] = "DMD Portal - " + Model.DocumentNumber;
    string[]? EN = TempData["EN"] as string[];
    TempData.Keep();

    string sesID = Model.SessionID!;

    string afterTrav1 = "Property No. / Fixed Asset No.";
    string afterTrav2 = "Description";
    string afterTrav3 = "Calibration Due Date";
    string afterTravTitle = "TEST EQUIPMENT LOG";

    if (Model.AfterTravlog == "C")
    {
        afterTrav1 = "Part Number";
        afterTrav2 = "Description";
        afterTrav3 = "Serial Number";
        afterTravTitle = "CONFIGURATION LOG";
    }
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ATS Engineering</title>
    <link href="~/css/site.css" rel="stylesheet" />
    <script src="~/js/site.js"></script>
    <script src="~/chart.js/chart.js"></script>
    <a href="~/lib/bootstrap/dist/css/bootstrap.min.css.map"></a>
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <a href="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js.map"></a>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <link href="~/datatables/datatable.min.css" rel="stylesheet" />
    <script src="~/datatables/datatable.min.js"></script>
    <style>
        .modal {
            z-index: 2002;
        }
    </style>
</head>
<body>
    <div class="loader" id="loadingIcon"></div>
    <div class="container-fluid flex-column">
        <div class="container-fluid-block w-100" style="z-index:2001">
            <div class="w-100">
                <div class="d-flex bg-primary justify-content-between" style="height:8vh">
                    <div class="d-flex align-items-center">
                        <div class="my-auto mx-3">
                            <img style="width:60px; height: 30px" src="~/pimeslogo.png" />
                        </div>
                        @if (!Model.WorkingStat)
                        {
                            <a class="btn btn-secondary mx-3" asp-controller="Home" asp-action="MTIList" asp-route-whichDoc="@Model.Product" asp-route-whichType="@Model.DocType">
                                <div class="d-flex justify-content-between align-items-center">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-arrow-left-square-fill" viewBox="0 0 16 16">
                                        <path d="M16 14a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V2a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v12zm-4.5-6.5H5.707l2.147-2.146a.5.5 0 1 0-.708-.708l-3 3a.5.5 0 0 0 0 .708l3 3a.5.5 0 0 0 .708-.708L5.707 8.5H11.5a.5.5 0 0 0 0-1z" />
                                    </svg>
                                    <div class="mx-2">
                                        Back
                                    </div>
                                </div>
                            </a>

                            <button class="btn bg-warning text-white mx-1" data-bs-toggle="modal" data-bs-target="#viewtravmodal" onclick="SeeTraveler()">
                                <div class="d-flex">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-file-earmark-text-fill my-auto" viewBox="0 0 16 16">
                                        <path d="M9.293 0H4a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h8a2 2 0 0 0 2-2V4.707A1 1 0 0 0 13.707 4L10 .293A1 1 0 0 0 9.293 0M9.5 3.5v-2l3 3h-2a1 1 0 0 1-1-1M4.5 9a.5.5 0 0 1 0-1h7a.5.5 0 0 1 0 1zM4 10.5a.5.5 0 0 1 .5-.5h7a.5.5 0 0 1 0 1h-7a.5.5 0 0 1-.5-.5m.5 2.5a.5.5 0 0 1 0-1h4a.5.5 0 0 1 0 1z" />
                                    </svg>
                                    <div class="mx-2 text-nowrap">
                                        See Traveler
                                    </div>
                                </div>
                            </button>

                            <script>
                                var travLoadChecker = false;

                                function SeeTraveler() {

                                    if (travLoadChecker === false) {
                                        document.getElementById('viewtravmodalbody').src = '@Url.Action("ViewTraveler", "DocGenerator")' + '?docNo=' + '@Model.DocumentNumber';
                                    }

                                    travLoadChecker = true;
                                }
                            </script>

                            <div class="modal fade" data-bs-backdrop="static" id="viewtravmodal">
                                <div class="modal-dialog modal-dialog-centered modal-fullscreen">
                                    <div class="modal-content">
                                        <div class="modal-header bg-primary">
                                            <div class="modal-title d-flex text-white">
                                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-easel-fill my-auto" viewBox="0 0 16 16">
                                                    <path d="M8.473.337a.5.5 0 0 0-.946 0L6.954 2H2a1 1 0 0 0-1 1v7a1 1 0 0 0 1 1h1.85l-1.323 3.837a.5.5 0 1 0 .946.326L4.908 11H7.5v2.5a.5.5 0 0 0 1 0V11h2.592l1.435 4.163a.5.5 0 0 0 .946-.326L12.15 11H14a1 1 0 0 0 1-1V3a1 1 0 0 0-1-1H9.046L8.473.337z" />
                                                </svg>
                                                <p class="fs-5 fw-bold my-auto mx-2">View Traveler</p>
                                            </div>
                                            <button class="btn-close" data-bs-dismiss="modal"></button>
                                        </div>
                                        <div class="modal-body">
                                            <div>
                                                <iframe class="w-100 border-3 border-dark" id="viewtravmodalbody" src="" style="height:85vh">Traveler Here</iframe>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }
                        else
                        {
                            <button class="btn btn-danger text-white mx-1" onclick="" id="twbtn">
                                <div class="d-flex">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-file-earmark-text-fill my-auto" viewBox="0 0 16 16">
                                        <path d="M9.293 0H4a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h8a2 2 0 0 0 2-2V4.707A1 1 0 0 0 13.707 4L10 .293A1 1 0 0 0 9.293 0M9.5 3.5v-2l3 3h-2a1 1 0 0 1-1-1M4.5 9a.5.5 0 0 1 0-1h7a.5.5 0 0 1 0 1zM4 10.5a.5.5 0 0 1 .5-.5h7a.5.5 0 0 1 0 1h-7a.5.5 0 0 1-.5-.5m.5 2.5a.5.5 0 0 1 0-1h4a.5.5 0 0 1 0 1z" />
                                    </svg>
                                    <div class="mx-2 text-nowrap">
                                        Session
                                    </div>
                                </div>
                            </button>
                            <button class="btn btn-warning mx-1 text-white" onclick="GetTravProgress('readonly')">
                                <div class="d-flex">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-file-earmark-text-fill my-auto" viewBox="0 0 16 16">
                                        <path d="M9.293 0H4a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h8a2 2 0 0 0 2-2V4.707A1 1 0 0 0 13.707 4L10 .293A1 1 0 0 0 9.293 0M9.5 3.5v-2l3 3h-2a1 1 0 0 1-1-1M4.5 9a.5.5 0 0 1 0-1h7a.5.5 0 0 1 0 1zM4 10.5a.5.5 0 0 1 .5-.5h7a.5.5 0 0 1 0 1h-7a.5.5 0 0 1-.5-.5m.5 2.5a.5.5 0 0 1 0-1h4a.5.5 0 0 1 0 1z" />
                                    </svg>
                                    <div class="mx-2 text-nowrap">
                                        Your Progress
                                    </div>
                                </div>
                            </button>
                        }
                        @{
                            if (EN[1] == "USER")
                            {
                                if (Model.WorkingStat)
                                {
                                    <button class="btn btn-success mx-2" data-bs-target="#pausemodal" data-bs-toggle="modal">
                                        <div class="d-flex justify-content-between align-items-center">
                                            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-pause-btn-fill" viewBox="0 0 16 16">
                                                <path d="M0 12V4a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v8a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2zm6.25-7C5.56 5 5 5.56 5 6.25v3.5a1.25 1.25 0 1 0 2.5 0v-3.5C7.5 5.56 6.94 5 6.25 5zm3.5 0c-.69 0-1.25.56-1.25 1.25v3.5a1.25 1.25 0 1 0 2.5 0v-3.5C11 5.56 10.44 5 9.75 5z" />
                                            </svg>
                                            <div class="mx-2">
                                                Pause
                                            </div>
                                        </div>
                                    </button>
                                }
                                else
                                {
                                    if (!Model.ObsoleteStat)
                                    {
                                        <button class="btn btn-success mx-2" type="button" data-bs-target="#confirmStart" data-bs-toggle="modal">
                                            <div class="d-flex justify-content-between align-items-center">
                                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-skip-start-circle-fill" viewBox="0 0 16 16">
                                                    <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM9.71 5.093 7 7.028V5.5a.5.5 0 0 0-1 0v5a.5.5 0 0 0 1 0V8.972l2.71 1.935a.5.5 0 0 0 .79-.407v-5a.5.5 0 0 0-.79-.407z" />
                                                </svg>
                                                <div class="mx-2">
                                                    Start
                                                </div>
                                            </div>
                                        </button>
                                    }
                                    else
                                    {
                                        <p class="text-danger fw-bold my-auto">
                                            This is an obsolete document.
                                        </p>

                                        <script>
                                            var marquee = document.getElementById('marq');
                                            marquee.innerHTML = 'This is an obsolete document, please contact your originator for further assistance.';
                                            marquee.style.color = 'red';
                                        </script>
                                    }
                                }
                            }
                        }

                    </div>

                    <div class="d-flex align-items-center">
                        @if (Model.WorkingStat)
                        {
                            <button type="button" class="btn btn-success mx-1" data-bs-toggle="modal" data-bs-target="#travmodal" id="travBtn">
                                <div class="d-flex justify-content-between align-items-center">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-clipboard-check-fill" viewBox="0 0 16 16">
                                        <path d="M6.5 0A1.5 1.5 0 0 0 5 1.5v1A1.5 1.5 0 0 0 6.5 4h3A1.5 1.5 0 0 0 11 2.5v-1A1.5 1.5 0 0 0 9.5 0h-3Zm3 1a.5.5 0 0 1 .5.5v1a.5.5 0 0 1-.5.5h-3a.5.5 0 0 1-.5-.5v-1a.5.5 0 0 1 .5-.5h3Z" />
                                        <path d="M4 1.5H3a2 2 0 0 0-2 2V14a2 2 0 0 0 2 2h10a2 2 0 0 0 2-2V3.5a2 2 0 0 0-2-2h-1v1A2.5 2.5 0 0 1 9.5 5h-3A2.5 2.5 0 0 1 4 2.5v-1Zm6.854 7.354-3 3a.5.5 0 0 1-.708 0l-1.5-1.5a.5.5 0 0 1 .708-.708L7.5 10.793l2.646-2.647a.5.5 0 0 1 .708.708Z" />
                                    </svg>
                                    <div class="mx-2 text-nowrap">
                                        Submit Traveler
                                    </div>
                                </div>
                            </button>
                            <button type="button" class="btn btn-danger text-nowrap mx-1" data-bs-toggle="modal" data-bs-target="#problemlogForm">
                                <div class="d-flex justify-content-between align-items-center">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-send-exclamation-fill" viewBox="0 0 16 16">
                                        <path d="M15.964.686a.5.5 0 0 0-.65-.65L.767 5.855H.766l-.452.18a.5.5 0 0 0-.082.887l.41.26.001.002 4.995 3.178 1.59 2.498C8 14 8 13 8 12.5a4.5 4.5 0 0 1 5.026-4.47L15.964.686Zm-1.833 1.89L6.637 10.07l-.215-.338a.5.5 0 0 0-.154-.154l-.338-.215 7.494-7.494 1.178-.471-.47 1.178Z" />
                                        <path d="M12.5 16a3.5 3.5 0 1 0 0-7 3.5 3.5 0 0 0 0 7Zm.5-5v1.5a.5.5 0 0 1-1 0V11a.5.5 0 0 1 1 0Zm0 3a.5.5 0 1 1-1 0 .5.5 0 0 1 1 0Z" />
                                    </svg>
                                    <div class="mx-2">
                                        Problem Log
                                    </div>
                                </div>
                            </button>
                        }
                        <button class="btn btn-warning text-white mx-3" type="button" data-bs-toggle="offcanvas" data-bs-target="#docs">
                            <div class="d-flex justify-content-between align-items-center">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-list" viewBox="0 0 16 16">
                                    <path fill-rule="evenodd" d="M2.5 12a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5z" />
                                </svg>
                                <div class="mx-2">
                                    Documents
                                </div>
                            </div>
                        </button>
                        <div class="my-auto mx-3">
                            <img style="width:60px; height: 30px" src="~/ats wbg.png" />
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="container-fluid w-100">
            @* <div class="position-absolute bg-transparent end-0" style="height:60px; width:25%"> *@
            </div>
            <iframe class="w-100 border-3 border-dark" id="MainDoc" style="height:90vh" src="@Url.Action("ShowDoc", "MTI", new {docunumber = Model.DocumentNumber, whichDoc = "MainDoc.pdf"})"></iframe>
        </div>
    </div>

    <script>
        $(document).ready(function () {
            $('#twbtn').click(function () {
                $('#twmodal').modal('show');
                loadPartialView();
            });

            function loadPartialView() {
                $.ajax({
                    url: '@Url.Action("TWPartial", "TransferWork")',
                    method: 'GET',
                    success: function (data) {
                        $('#twpartialhere').html(data);
                    },
                    error: function (error) {
                        console.log(error);
                    }
                });
            }
        });
    </script>

    <div class="modal fade" data-bs-backdrop="static" id="twmodal" style="z-index:2002">
        <div class="modal-dialog modal-xl">
            <div class="modal-content">
                <div class="modal-header bg-primary">
                    <p class="fw-bold fs-5 modal-title text-white">On-Going and Pending sessions</p>
                    <button class="btn-close" data-bs-dismiss="modal" onclick="$('#twpartialhere').html('');"></button>
                </div>
                <div class="modal-body" id="twpartialhere" style="height:75vh">

                </div>
                <div class="modal-footer">
                    <a class="btn btn-danger" asp-controller="Work" asp-action="HoldSession" asp-route-sessionID="@Model.SessionID">Hold Session</a>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="travprogressedit" data-bs-backdrop="static" style="z-index: 2003">
        <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable modal-xl">
            <div class="modal-content">
                <div class="modal-header bg-primary">
                    <p class="modal-title text-white fs-5 fw-bold">Progress</p>
                    <button class="btn-close" data-bs-dismiss="modal"></button>
                </div>
                <form asp-controller="Document" asp-action="SaveTravChanges" id="edittravform" asp-route-sessionId="@Model.SessionID">
                    <div class="modal-body" style="height: 520px">
                        <div class="table-responsive">
                            <table class="table table-bordered table-striped">
                                <thead>
                                    <tr>
                                        <th>
                                            Step
                                        </th>
                                        <th class="w-50">
                                            Instruction
                                        </th>
                                        <th>
                                            Parameter
                                        </th>
                                    </tr>
                                </thead>
                                <tbody id="editprevtravbody">

                                </tbody>
                            </table>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-primary" type="submit" id="subbutton">Submit</button>
                    </div>
                </form>
            </div>
        </div>
    </div>

    <script>
        $(function (){
            $('#edittravform').submit(function (e){
                e.preventDefault();

                $.ajax({
                    url: $(this).attr('action'),
                    data: $(this).serialize(),
                    success: function (response) {
                        
                    },
                    error: function (error) {
                        
                    }
                });

                $('#travprogressedit').modal('hide');
            });
        });

        function GetTravProgress(sub)
        {
            $(function () {

                var DoneSig = '';
                var editprogbody = document.getElementById('editprevtravbody');
                var subb = document.getElementById('subbutton');
                editprogbody.innerHTML = '';

                if (sub === 'readonly' || '@Model.DocType' === 'MPI') {
                    subb.style.display = 'none';
                }
                else
                {
                    subb.style.display = 'block';
                }

                $('#travprogressedit').modal('show');

                $.ajax({
                    url: '@Url.Action("GetTravDataForEdit", "Traveler")',
                    data: {sessionId: '@Model.SessionID', doctype: '@Model.DocType'},
                    success: function (r){
                        r.r.forEach(function(line){

                            if ('@Model.DocType' === 'MPI') {
                                sub = 'readonly';
                                line.SinglePara = 'Done';
                            }

                            if (line.isMerge === true){
                                editprogbody.innerHTML += '<tr><td><input name="Step" class="form-control" value="' + line.Step + '" readonly /></td><td><textarea name="Instruction" class="form-control" readonly>' + line.Instruction + '</textarea></td><td><input name="SinglePara" placeholder="Required" class="form-control" value="' + line.SinglePara + '" ' + sub + ' /><input name="isMerge" value="' + line.isMerge + '" hidden /><input name="FirstThreePara" value="" hidden/><input name="SecondThreePara" value="" hidden/><input name="ThirdThreePara" value="" hidden/></td></tr>';
                            }
                            else{
                                editprogbody.innerHTML += '<tr><td><input name="Step" class="form-control" value="' + line.Step + '" readonly /></td><td><textarea name="Instruction" class="form-control" readonly>' + line.Instruction + '</textarea></td><td><div class="d-flex"><input name="FirstThreePara" class="form-control" value="' + line.FirstThreePara + '" ' + sub + ' /><input name="SecondThreePara" class="form-control" value="' + line.SecondThreePara + '" ' + sub + ' /><input name="ThirdThreePara" class="form-control" value="' + line.ThirdThreePara + '" ' + sub + ' /></div><input name="isMerge" value="' + line.isMerge + '" hidden /></td> <input name="SinglePara" value="" hidden/></tr>';
                            }

                        });
                    }
                });
            });
        }
    </script>

    <div class="modal fade" id="confirmStart" data-bs-backdrop="static">
        <div class="modal-dialog modal-dialog-centered modal-sm">
            <form  id="startworkform">
                <div class="modal-content">
                    <div class="modal-header d-flex justify-content-start bg-primary text-white">
                        <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="currentColor" class="bi bi-question-diamond-fill my-auto" viewBox="0 0 16 16">
                            <path d="M9.05.435c-.58-.58-1.52-.58-2.1 0L.436 6.95c-.58.58-.58 1.519 0 2.098l6.516 6.516c.58.58 1.519.58 2.098 0l6.516-6.516c.58-.58.58-1.519 0-2.098L9.05.435zM5.495 6.033a.237.237 0 0 1-.24-.247C5.35 4.091 6.737 3.5 8.005 3.5c1.396 0 2.672.73 2.672 2.24 0 1.08-.635 1.594-1.244 2.057-.737.559-1.01.768-1.01 1.486v.105a.25.25 0 0 1-.25.25h-.81a.25.25 0 0 1-.25-.246l-.004-.217c-.038-.927.495-1.498 1.168-1.987.59-.444.965-.736.965-1.371 0-.825-.628-1.168-1.314-1.168-.803 0-1.253.478-1.342 1.134-.018.137-.128.25-.266.25h-.825zm2.325 6.443c-.584 0-1.009-.394-1.009-.927 0-.552.425-.94 1.01-.94.609 0 1.028.388 1.028.94 0 .533-.42.927-1.029.927z" />
                        </svg>
                        <p class="fs-5 fw-bold my-auto mx-2">Confirm</p>
                    </div>
                    <div class="modal-body flex-column">
                        <div>
                            <label class="form-label">Work Order:</label>
                            <input class="form-control" name="wOrder" id="workorder" required />
                        </div>
                        <div>
                            <label class="form-label">Module:</label>
                            <input class="form-control" name="module" id="module" required />
                        </div>
                        <div>
                            <label class="form-label">Serial No:</label>
                            <input class="form-control" name="serialNo" id="serialno" required />
                        </div>                       
                    </div>
                    <div class="modal-footer">
                        <div class="d-flex justify-content-between w-100">
                            <div>
                                <button class="btn btn-danger" type="button" data-bs-dismiss="modal">Cancel</button>
                            </div>
                            <div>
                                <input class="btn btn-primary" type="submit"/>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>

    <script>
        $(function (){
            $('#startworkform').submit(function (e){
                e.preventDefault();

                var workOrder = document.getElementById('workorder').value;
                var serialNo = document.getElementById('serialno').value;
                var Module = document.getElementById('module').value;

                $.ajax({
                    url: '@Url.Action("ValidateModule", "Work")',
                    data: {module: Module, serialNo: serialNo, workOrder: workOrder},
                    success: function (response){

                        if (response.response === 'go')
                        {
                            window.location.href = '@Url.Action("StartWork", "Work")' + '?docno=' + '@Model.DocumentNumber' + '&EN=' + '@EN[0]' + '&wOrder=' + workOrder + '&serialNo=' + serialNo + '&module=' + Module + '&docType=' + '@Model.DocType';
                        }
                        else{
                            alert(response.message);
                            $('#workorder').val('');
                            $('#serialno').val('');
                            $('#module').val('');
                        }
                    }
                });
            });
        });
    </script>

    <div class="offcanvas offcanvas-end border-3 shadow" style="z-index:2001; background-color:#B5AD9A" data-bs-backdrop="static" id="docs">
        <div class="offcanvas-header bg-primary border-5 border-dark">
            <div class="d-flex justify-content-center w-100">
                <h5 class="offcanvas-title text-center text-white fw-bolder" id="offcanvasExampleLabel">Documents</h5>
            </div>
            <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" id="closeoffcanvas"></button>
        </div>
        <div class="offcanvas-body overflow-auto flex-column">
            <div class="w-100 h-100">
                <div class="card border-3 border-dark">
                    <div class="card-header text-white border-3 border-dark" style="background-color:darkslateblue">
                        <h4 class="card-title text-center">Primary Docs</h4>
                    </div>
                    <div class="card-body flex-column justify-content-center w-100">
                        <div class="d-flex justify-content-center">
