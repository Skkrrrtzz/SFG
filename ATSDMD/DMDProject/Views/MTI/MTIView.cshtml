    <script>
        $(function () {
            $('#submitTravForm').submit(function (event) {
                event.preventDefault();
                document.getElementById('loadingIcon').style.display = 'block';

                var stepNumber = document.getElementById('stepnoid').value;
                var Task = document.getElementById('taskid').value;
                var Single = document.getElementById('singleParaid').value;
                var Div = [];

                if (divType === 't') {
                    Div.push(document.getElementById('firstParaid').value);
                    Div.push(document.getElementById('secParaid').value);
                    Div.push(document.getElementById('thirdParaid').value);
                }

                if (Single === '' && Div.length === 0) {
                    return;
                }

                $.ajax({
                    type: 'POST',
                    url: $(this).attr('action'),
                    data: { stepNo: stepNumber, tAsk: Task, byThree: Div, singlePara: Single, sessionID: '@sesID', tech: '@EN[0]', date: '@DateTime.Now.ToShortDateString()', docType: '@Model.DocType' },
                    success: function (response) {
                        document.getElementById('closetravmodal').click();
                        DisableTravBtn();
                        if (response.Task === "" || response.Task === null) {
                            if ('@Model.AfterTravlog' !== 'N') {
                                $('#startlogsession').modal('show');
                            }
                            else {
                                $('#endsession').modal('show');
                            }

                        }
                        else {
                            $('#stepnoid').val(response.StepNo);
                            $('#taskid').val(response.Task);
                            $('#firstParaid').val('');
                            $('#secParaid').val('');
                            $('#thirdParaid').val('');
                            $('#singleParaid').val('');

                            divType = response.Div;
                            var Sing = document.getElementById('single');
                            var Thr = document.getElementById('three');

                            if (response.Div === 't') {
                                Sing.style.display = 'none';
                                Thr.style.display = 'block';
                                document.getElementById('firstParaid').setAttribute('required', '');
                                document.getElementById('secParaid').setAttribute('required', '');
                                document.getElementById('thirdParaid').setAttribute('required', '');
                                document.getElementById('singleParaid').removeAttribute('required');
                            }
                            else {
                                Sing.style.display = 'block';
                                Thr.style.display = 'none';
                                document.getElementById('firstParaid').removeAttribute('required');
                                document.getElementById('secParaid').removeAttribute('required');
                                document.getElementById('thirdParaid').removeAttribute('required');
                                document.getElementById('singleParaid').setAttribute('required', '');
                            }
                        }

                    },
                    error: function (error) {
                        console.error('An error occurred: ' + error.statusText);
                    }
                });

                document.getElementById('loadingIcon').style.display = 'none';
            });
        });

        document.querySelectorAll('input').forEach(function (input) {
            input.setAttribute("autocomplete", "off");
        });

        window.onload = function () {
            $(function () {
                $.ajax({
                    url: '@Url.Action("UserRefreshed", "Work")',
                    data: { sessionId: '@Model.SessionID', docType: '@Model.DocType'},
                    success: function (response) {

                        $('#stepnoid').val(response.StepNo);
                        $('#taskid').val(response.Task);

                        if (response.StepNo === null && '@Model.WorkingStat' === 'True') {
                            if ('@Model.AfterTravlog' !== 'N') {
                                $('#startlogsession').modal('show');
                            }
                            else {
                                $('#endsession').modal('show');
                            }
                        }
                        else {
                            var Sing = document.getElementById('single');
                            var Thr = document.getElementById('three');
                            divType = response.Div;

                            if (response.Div === 't') {
                                Sing.style.display = 'none';
                                Thr.style.display = 'block';

                                document.getElementById('firstParaid').setAttribute('required', '');
                                document.getElementById('secParaid').setAttribute('required', '');
                                document.getElementById('thirdParaid').setAttribute('required', '');
                                document.getElementById('singleParaid').removeAttribute('required');
                            }
                            else {
                                Sing.style.display = 'block';
                                Thr.style.display = 'none';

                                document.getElementById('firstParaid').removeAttribute('required');
                                document.getElementById('secParaid').removeAttribute('required');
                                document.getElementById('thirdParaid').removeAttribute('required');
                                document.getElementById('singleParaid').setAttribute('required', '');
                            }
                        }

                        if ('@Model.DocType' === 'MPI') {
                            var sing = document.getElementById('singleParaid');
                            sing.setAttribute('required', '');
                            sing.setAttribute('hidden', '');
                            document.getElementById('submittravbtn').setAttribute('hidden', '');
                            document.getElementById('three').style.display = 'none';
                            document.getElementById('single').style.display = 'block';
                        }
                    }
                });
            });
        };

