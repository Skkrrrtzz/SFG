    <script>
        $(function (){
            $('#editconfigform').submit(function (e){
                e.preventDefault();

                $.ajax({
                    url: $(this).attr('action'),
                    data: $(this).serialize(),
                    success: function (r){

                    }
                });

                $('#configprogressedit').modal('hide');
            });
        });

        function GetConfigProgress(type) {
            if (type === 'readonly'){
                document.getElementById('configbutton').style.display = 'none';
            }
            else{
                document.getElementById('configbutton').style.display = 'block';
            }

            var configbody = document.getElementById('configeditbody');
            configbody.innerHTML = '';
            
            $(function () {

                $('#configprogressedit').modal('show');

                $.ajax({
                    url: '@Url.Action("GetLogsheetDataForEdit", "Document")',
                    data: { sessionId: '@Model.SessionID', logType: '@Model.AfterTravlog' },
                    success: function (r) {
                        r.r.forEach(function (line) {
                            
                            configbody.innerHTML += '<tr><td><input class="form-control" name="PN" value="' + line.PN + '" ' + type + ' required/></td><td><input class="form-control" name="Desc" value="' + line.Desc + '" ' + type + ' required/></td><td><input class="form-control" name="Parameter" value="' + line.Parameter + '" ' + type + ' required/></td></tr>'
                        });
                    }
                });

                $('#configprogressedit').modal('hide');
            });
        }
    </script>

