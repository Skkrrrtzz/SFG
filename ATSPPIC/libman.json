{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.7.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "bootstrap@5.3.3",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "jqueryui@1.13.3",
      "destination": "wwwroot/lib/jqueryui/"
    },
    {
      "library": "bootstrap-icons@1.11.3",
      "destination": "wwwroot/lib/bootstrap-icons/",
      "files": [
        "font/fonts/bootstrap-icons.woff",
        "font/fonts/bootstrap-icons.woff2",
        "font/bootstrap-icons.css",
        "font/bootstrap-icons.json",
        "font/bootstrap-icons.min.css"
      ]
    },
    {
      "library": "jquery-ajax-unobtrusive@3.2.6",
      "destination": "wwwroot/lib/jquery-ajax-unobtrusive/"
    },
    {
      "library": "font-awesome@6.5.2",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "provider": "jsdelivr",
      "library": "datatables.net@2.0.7",
      "destination": "wwwroot/lib/datatables.net/",
      "files": [
        "js/dataTables.js",
        "js/dataTables.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "datatables.net-bs5@2.0.7",
      "destination": "wwwroot/lib/datatables.net-bs5/"
    }
  ]
}