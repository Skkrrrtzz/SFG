@page
@model ATSSFG.Pages.Dashboard.OpenProjectsModel

<div class="card p-2 bg-light">
    <div class="border-3 border-bottom border-dark">
        <h4 class="fw-bold">Open Projects</h4>
    </div>
    <div class="table-responsive p-2">
        <table class="table table-striped table-hover display nowrap" style="width:100%" id="openProjectsTbl">
            <thead class="table-secondary">
                <tr>
                    <th>Project Name</th>
                    <th>Quotation Code</th>
                    <th>Date Requested</th>
                    <th>Required Submission Date</th>
                    <th>No of Items</th>
                    <th>No of Items with price</th>
                    <th>No of Items without price</th>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
</div>

@section Scripts {
    <script src="~/JavaScript/OpenProjects.js"></script>
}
